<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.spring.mapper.memberMapper">
 
 <insert id="register">
 	INSERT INTO member(id_member,pw_member,name_member,age_member,gender_member,address_member,phone_member) 
 		VALUES(#{memberId},#{memberPw},#{memberName},#{memberAge},#{memberGender},#{memberAddress},#{memberPhone})
 </insert> 
 
 
<select id="login" resultType="com.spring.dto.MemberVO">
        SELECT id_member, name_member,phone_member FROM member WHERE id_member = #{memberId} and pw_member = #{memberPw}
    </select>
    
</mapper>
